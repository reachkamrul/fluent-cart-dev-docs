					}
				},
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{base_url}}/wp-json/fluent-cart/v2/taxes/?per_page=10&page=1",
					"host": ["{{base_url}}"],
					"path": ["wp-json", "fluent-cart", "v2", "taxes", ""],
					"query": [
						{
							"key": "search",
							"value": "",
							"disabled": true
						},
						{
							"key": "per_page",
							"value": "10"
						},
						{
							"key": "page",
							"value": "1"
						},
						{
							"key": "filed",
							"value": "",
							"disabled": true
						},
						{
							"key": "country",
							"value": "",
							"disabled": true
						}
					]
				},
				"description": "Retrieve a paginated list of tax records."
			},
			"response": []
		},
		{
			"name": "Mark Taxes as Filed",
			"request": {
				"auth": {
					"type": "basic",
					"basic": {
						"password": "{{password}}",
						"username": "{{username}}"
					}
				},
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"ids\": [1, 2, 3, 4, 5]\n}"
				},
				"url": {
					"raw": "{{base_url}}/wp-json/fluent-cart/v2/taxes/",
					"host": ["{{base_url}}"],
					"path": ["wp-json", "fluent-cart", "v2", "taxes", ""]
				},
				"description": "Mark one or more tax records as filed."
			},
			"response": []
		}
	],
	"variable": []
